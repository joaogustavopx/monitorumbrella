{
    "app": {
      "name": "QRCodeScannerApp",
      "components": {
        "HomeScreen": {
          "image": {
            "uri": "https://manufacturingskillsinstitute.org/wp-content/uploads/2021/07/Stihl-Logo-for-website.png",
            "style": {
              "width": 200,
              "height": 200,
              "marginBottom": 20
            }
          },
          "text": {
            "content": "Leia o QRcode",
            "style": {
              "marginBottom": 20,
              "textAlign": "center"
            }
          },
          "button": {
            "title": "Ler Código QR",
            "navigationTarget": "QRCodeScreen"
          },
          "statusBarStyle": "auto"
        },
        "QRCodeScreen": {
          "permissions": {
            "cameraPermissionStatus": null,
            "requestPermissionsAsyncLog": true
          },
          "scannedData": {
            "scanned": false,
            "data": ""
          },
          "messages": {
            "requestingPermission": "Solicitando permissão da câmera...",
            "noAccessToCamera": "Sem acesso à câmera. Verifique as permissões nas configurações do dispositivo."
          },
          "buttonScanAgain": {
            "title": "Escanear novamente",
            "actionOnPress": true
          },
          "statusBarStyle": "auto"
        }
      },
      "styles": {
        "containerStyle": {
          "flex": 1,
          "backgroundColor": "#f46717",
          "alignItems": "center",
          "justifyContent": "center"
        },
        "android": {
      "package": "com.aexemplo.gerencoamentoumbrella",
      "versionCode": 1
    },
        "textStyle": {
        }
      }
    }
  }
  